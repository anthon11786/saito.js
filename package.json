{
  "name": "saitojs-client",
  "version": "0.1.0",
  "description": "Lightweight JavaScript library for Saito Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:rest": "ts-node examples/rest-only-test.ts",
    "test:live": "ts-node examples/live-test.ts",
    "test:basic": "ts-node examples/basic-usage.ts",
    "test:chat": "ts-node examples/chat-example.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "saito",
    "blockchain",
    "web3",
    "client",
    "sdk"
  ],
  "author": "Saito Network",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saito-network/saito.js"
  },
  "dependencies": {
    "base-58": "^0.0.1",
    "json-bigint": "^1.0.0",
    "tweetnacl": "^1.0.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/json-bigint": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}